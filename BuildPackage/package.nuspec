<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
    <metadata>
        <id>DiploTraceLogViewer</id>
        <version>0.0.0</version>
        <title>Diplo Trace Log Viewer for Umbraco</title>
        <authors>Dan Booth</authors>
        <owners>Dan Booth</owners>
        <projectUrl>http://our.umbraco.org/projects/developer-tools/diplo-trace-log-viewer</projectUrl>
